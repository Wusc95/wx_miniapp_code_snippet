.container{
  border-radius:50%;
  box-sizing: border-box;
  position: relative;
  overflow:hidden;
}
.container_in{
  width:100%;
  height:100%;
  overflow:hidden;
  position:absolute;
  top:0;
}
.progress_img{
  position: absolute;
  top:0;
  left:0;
}
.progress_value{
  position:absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  display:flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size:30rpx;
  color:red;
}